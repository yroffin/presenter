{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "styles",
  "info": {
    "name": "style"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "body": {
      "type": "richtext"
    },
    "slide": {
      "via": "styles",
      "model": "slide"
    }
  }
}
